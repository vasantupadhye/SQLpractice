SELECT prod_category,
       order_total,
       emp_name,
       order_num
FROM oliveoil.sales
WHERE prod_category != 'Bath_products' , 'olive oil'
ORDER BYE produ_category DESC
LIMIT 5;


#or

SELECT prod_category
       order_total
       emp_name
       order_num
FROM oliveoil.sales
WHERE prod_category NOT IN ('Bath_Products' , 'oliveoil')
ORDER BY prod_category DESC
LIMIT 5;
